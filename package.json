{
  "name": "typescript-react-starter-kit",
  "version": "0.1.0",
  "description": "React Starter Kit",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "build-storybook": "build-storybook",
    "start": "webpack-dev-server --config webpack.dev.config.js",
    "lint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "test": "jest --updateSnapshot",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand",
    "test:watch": "npm test -- --watch",
    "pretest:e2e": "npm run build",
    "http-server": "http-server dist",
    "test:e2e": "start-server-and-test http-server http://localhost:8080 cypress:run",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,json,css,md}": [
      "prettier --config ./.prettierrc --write",
      "git add"
    ],
    "src/**/*.{js,ts,tsx}": [
      "tslint -c ./tslint.json --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:inkless/react-starter-kit.git"
  },
  "author": {
    "name": "Guangda Zhang",
    "email": "zhangxiaoyu9350@gmail.com"
  },
